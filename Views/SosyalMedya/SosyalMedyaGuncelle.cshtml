@model BusranurCirikCv.Models.Entity.TblSosyalMedya

@{
    ViewBag.Title = "SosyalMedyaGuncelle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
    <h4>Sosyal Medya Düzenleme</h4>
    @Html.Label("Sosyal Medya Adı")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    @Html.Label("Sosyal Medya Linki")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    @Html.Label("Sosyal Medya İkonu")
    @Html.TextBoxFor(x => x.İkon, new { @class = "form-control" })
    <br />
    <button class="btn btn-dark">Güncelle</button>
</form>

